<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="1050.0" xmlns="http://javafx.com/javafx/8.0.172-ea"
      xmlns:fx="http://javafx.com/fxml/1">
    <AnchorPane fx:id="utilPane" maxWidth="1500.0" prefHeight="100.0" prefWidth="1020.0"
                stylesheets="/com/bht/pim/fragment/children/project/ProjectUtilFragment.css"
                xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
        <Button fx:id="bNew" layoutX="954.0" layoutY="5.0" mnemonicParsing="false" prefHeight="96.0" prefWidth="96.0"
                AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="18.0">
            <font>
                <Font size="20.0"/>
            </font>
        </Button>
        <TextField fx:id="searchBox" layoutX="34.0" layoutY="28.0" prefHeight="36.0" prefWidth="480.0"
                   promptText="Enter Keyword to search" AnchorPane.bottomAnchor="60.0" AnchorPane.leftAnchor="30.0"
                   AnchorPane.rightAnchor="460.0">
            <font>
                <Font size="16.0"/>
            </font>
        </TextField>
        <ComboBox fx:id="comboBoxStatus" layoutX="604.0" layoutY="28.0" prefHeight="36.0" prefWidth="180.0"
                  promptText="STATUS" AnchorPane.bottomAnchor="60.0" AnchorPane.rightAnchor="250.0"/>
        <Label fx:id="lNumberOfProjects" layoutX="30.0" layoutY="64.0" text="0" AnchorPane.bottomAnchor="5.0"
               AnchorPane.leftAnchor="36.0">
            <font>
                <Font name="System Bold" size="25.0"/>
            </font>
        </Label>
        <Label fx:id="lSelected" layoutX="60.0" layoutY="64.0" text="project(s) selected" AnchorPane.bottomAnchor="10.0"
               AnchorPane.leftAnchor="70.0">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Button fx:id="bDeleteAll" layoutX="390.0" layoutY="60.0" mnemonicParsing="false" prefHeight="30.0"
                prefWidth="180.0" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="250.0">
            <font>
                <Font size="14.0"/>
            </font>
        </Button>
        <Separator layoutX="30.0" layoutY="59.0" prefWidth="200.0" AnchorPane.bottomAnchor="45.0"
                   AnchorPane.leftAnchor="30.0" AnchorPane.rightAnchor="250.0"/>
        <Button fx:id="bReset" layoutX="964.0" layoutY="15.0" mnemonicParsing="false" prefHeight="96.0" prefWidth="96.0"
                AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="125.0">
            <font>
                <Font size="20.0"/>
            </font>
        </Button>
    </AnchorPane>
</VBox>
