<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<VBox fx:id="mainLabel" alignment="CENTER" maxHeight="60.0" minHeight="60.0" prefHeight="60.0" prefWidth="1050.0"
      stylesheets="/com/bht/pim/fragment/children/label/MainLabel.css" xmlns="http://javafx.com/javafx/8.0.999-ea"
      xmlns:fx="http://javafx.com/fxml/1">

    <Label fx:id="label" alignment="CENTER" text="MAIN LABEL" textFill="#606060" VBox.vgrow="ALWAYS">
        <font>
            <Font name="Segoe UI Bold" size="28.0"/>
        </font>
    </Label>
    <Separator prefWidth="200.0" VBox.vgrow="ALWAYS"/>
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
</VBox>
