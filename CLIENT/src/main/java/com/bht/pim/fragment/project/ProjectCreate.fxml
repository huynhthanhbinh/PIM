<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<VBox xmlns:fx="http://javafx.com/fxml/1" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity"
      minWidth="-Infinity"
      prefHeight="630.0" prefWidth="1120.0" spacing="5.0"
      stylesheets="/com/bht/pim/fragment/project/ProjectCreate.css" xmlns="http://javafx.com/javafx/8.0.172-ea"
      fx:controller="com.bht.pim.fragment.project.ProjectCreate">
    <padding>
        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
    </padding>
    <children>
        <VBox alignment="CENTER" prefHeight="60.0" prefWidth="1110.0">
            <children>
                <Label alignment="CENTER" text="NEW PROJECT" VBox.vgrow="ALWAYS">
                    <font>
                        <Font name="System Bold" size="24.0"/>
                    </font>
                </Label>
                <Label text="( * ) mandatory fields" textFill="#f24517">
                    <font>
                        <Font name="System Bold Italic" size="16.0"/>
                    </font>
                </Label>
                <Separator prefWidth="200.0"/>
            </children>
        </VBox>
        <GridPane alignment="CENTER" hgap="30.0" VBox.vgrow="ALWAYS">
            <columnConstraints>
                <ColumnConstraints hgrow="NEVER" maxWidth="141.0" minWidth="10.0" prefWidth="139.0"/>
                <ColumnConstraints hgrow="NEVER" maxWidth="254.0" minWidth="10.0" prefWidth="203.0"/>
                <ColumnConstraints hgrow="NEVER" maxWidth="203.0" minWidth="0.0" prefWidth="83.0"/>
                <ColumnConstraints hgrow="NEVER" maxWidth="292.0" minWidth="10.0" prefWidth="202.0"/>
                <ColumnConstraints hgrow="NEVER" maxWidth="270.0" minWidth="10.0" prefWidth="193.0"/>
                <ColumnConstraints hgrow="NEVER" maxWidth="270.0" minWidth="10.0" prefWidth="80.0"/>
                <ColumnConstraints/>
            </columnConstraints>
            <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="80.0" vgrow="NEVER"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER"/>
                <RowConstraints minHeight="10.0" prefHeight="50.0" vgrow="NEVER"/>
            </rowConstraints>
            <children>
                <Pane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.columnSpan="4"/>
                <Pane prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="4" GridPane.columnSpan="2">
                    <children>
                        <Label layoutX="1.0" layoutY="19.0" text="PROJECT'S MEMBERS">
                            <font>
                                <Font name="System Bold" size="24.0"/>
                            </font>
                        </Label>
                    </children>
                </Pane>
                <VBox prefHeight="46.0" prefWidth="143.0" GridPane.columnSpan="7" GridPane.rowIndex="11">
                    <children>
                        <Separator prefWidth="200.0"/>
                        <HBox alignment="CENTER" prefHeight="114.0" prefWidth="1110.0" spacing="20.0">
                            <children>
                                <Button defaultButton="true" graphicTextGap="10.0" mnemonicParsing="false"
                                        prefHeight="25.0" prefWidth="180.0" text="CREATE" HBox.hgrow="ALWAYS">
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </Button>
                                <Button cancelButton="true" graphicTextGap="10.0" mnemonicParsing="false"
                                        prefHeight="25.0" prefWidth="180.0" text="CANCEL" HBox.hgrow="ALWAYS">
                                    <font>
                                        <Font size="16.0"/>
                                    </font>
                                </Button>
                            </children>
                        </HBox>
                    </children>
                </VBox>
                <Label text="End-date" GridPane.columnIndex="2" GridPane.rowIndex="9">
                    <font>
                        <Font size="16.0"/>
                    </font>
                </Label>
                <DatePicker prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="1" GridPane.hgrow="NEVER"
                            GridPane.rowIndex="9"/>
                <DatePicker prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.hgrow="NEVER"
                            GridPane.rowIndex="9"/>
                <ComboBox prefHeight="40.0" prefWidth="200.0" promptText="Choose option" GridPane.columnIndex="1"
                          GridPane.hgrow="NEVER" GridPane.rowIndex="7" fx:id="comboBoxOption"/>
                <ComboBox prefHeight="40.0" prefWidth="200.0" promptText="Choose leader" GridPane.columnIndex="3"
                          GridPane.hgrow="NEVER" GridPane.rowIndex="7"/>
                <TextField prefHeight="40.0" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.hgrow="NEVER"
                           GridPane.rowIndex="5" GridPane.valignment="CENTER" GridPane.vgrow="NEVER"
                           promptText="Input customer name" fx:id="customer"/>
                <TextField prefHeight="40.0" GridPane.columnIndex="1" GridPane.columnSpan="3" GridPane.hgrow="NEVER"
                           GridPane.rowIndex="3" GridPane.valignment="CENTER" GridPane.vgrow="NEVER"
                           promptText="Input project name" fx:id="name"/>
                <TextField prefHeight="40.0" GridPane.columnIndex="1" GridPane.hgrow="NEVER" GridPane.rowIndex="1"
                           promptText="Input number only" fx:id="number"/>
                <ComboBox prefHeight="40.0" prefWidth="200.0" GridPane.columnIndex="3" GridPane.hgrow="NEVER"
                          GridPane.rowIndex="1" fx:id="comboBoxStatus"/>
                <Label alignment="TOP_LEFT" text="BASIC INFORMATION" GridPane.columnSpan="2">
                    <font>
                        <Font name="System Bold" size="24.0"/>
                    </font>
                </Label>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="1">
                    <children>
                        <Label text="Project-number">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label text="* " textFill="#f24517">
                            <font>
                                <Font name="System Bold Italic" size="22.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="3">
                    <children>
                        <Label text="Project-name">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label text="* " textFill="#f24517">
                            <font>
                                <Font name="System Bold Italic" size="22.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="2"
                      GridPane.rowIndex="1">
                    <children>
                        <Label text="Status">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label text="* " textFill="#f24517">
                            <font>
                                <Font name="System Bold Italic" size="22.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="5">
                    <children>
                        <Label text="Customer">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label text="* " textFill="#f24517">
                            <font>
                                <Font name="System Bold Italic" size="22.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="7">
                    <children>
                        <Label text="Group-option">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label text="* " textFill="#f24517">
                            <font>
                                <Font name="System Bold Italic" size="22.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.columnIndex="2"
                      GridPane.rowIndex="7">
                    <children>
                        <Label text="Leader">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label text="*" textFill="#f24517">
                            <font>
                                <Font name="System Bold Italic" size="22.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <HBox alignment="CENTER_LEFT" prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="9">
                    <children>
                        <Label text="Start-date">
                            <font>
                                <Font size="16.0"/>
                            </font>
                        </Label>
                        <Label text="* " textFill="#f24517">
                            <font>
                                <Font name="System Bold Italic" size="22.0"/>
                            </font>
                        </Label>
                    </children>
                </HBox>
                <TextField fx:id="textField" prefHeight="40.0" promptText="Search for employee to add"
                           GridPane.columnIndex="4" GridPane.columnSpan="2" GridPane.rowIndex="1"/>
                <Label fx:id="lNumber" text="Number of members : " GridPane.columnIndex="4" GridPane.rowIndex="2">
                    <font>
                        <Font name="System Bold" size="18.0"/>
                    </font>
                </Label>
                <Label fx:id="lSize" text="0" textFill="#f24517" GridPane.columnIndex="5" GridPane.rowIndex="2">
                    <font>
                        <Font name="System Bold" size="22.0"/>
                    </font>
                </Label>
                <TableView fx:id="table" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="4"
                           GridPane.columnSpan="2" GridPane.rowIndex="3" GridPane.rowSpan="7">
                    <columns>
                        <TableColumn fx:id="cName" prefWidth="227.0" text="NAME"/>
                        <TableColumn fx:id="cRemove" minWidth="9.0" prefWidth="51.0"/>
                    </columns>
                </TableView>
                <Label text="Project-number already exist !" textFill="#f24517" GridPane.columnIndex="1"
                       GridPane.rowIndex="2" fx:id="lNumberExist">
                    <font>
                        <Font name="System Bold Italic" size="14.0"/>
                    </font>
                </Label>
                <Label text="Project-name cannot be empty !" textFill="#f24517" GridPane.columnIndex="1"
                       GridPane.columnSpan="2" GridPane.rowIndex="4">
                    <font>
                        <Font name="System Bold Italic" size="14.0"/>
                    </font>
                </Label>
                <Label text="Customer cannot be empty !" textFill="#f24517" GridPane.columnIndex="1"
                       GridPane.rowIndex="6">
                    <font>
                        <Font name="System Bold Italic" size="14.0"/>
                    </font>
                </Label>
                <Label text="Please choose group-leader !" textFill="#f24517" GridPane.columnIndex="3"
                       GridPane.rowIndex="8">
                    <font>
                        <Font name="System Bold Italic" size="14.0"/>
                    </font>
                </Label>
                <Label text="Please specify Start-date !" textFill="#f24517" GridPane.columnIndex="1"
                       GridPane.rowIndex="10">
                    <font>
                        <Font name="System Bold Italic" size="14.0"/>
                    </font>
                </Label>
                <Label text="End-date is after Start-date ! " textFill="#f24517" GridPane.columnIndex="3"
                       GridPane.rowIndex="10">
                    <font>
                        <Font name="System Bold Italic" size="14.0"/>
                    </font>
                </Label>
            </children>
            <padding>
                <Insets left="5.0" right="5.0"/>
            </padding>
            <VBox.margin>
                <Insets left="10.0" right="10.0"/>
            </VBox.margin>
        </GridPane>
    </children>
</VBox>
