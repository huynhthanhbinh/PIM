<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            prefHeight="50.0" prefWidth="1050.0">
    <Button fx:id="bNew" mnemonicParsing="false" text="NEW"
            AnchorPane.rightAnchor="20.0" AnchorPane.topAnchor="5.0">
        <font>
            <Font name="System Bold" size="20.0"/>
        </font>
    </Button>
    <Button fx:id="bDelete" mnemonicParsing="false" text="DELETE"
            AnchorPane.leftAnchor="20.0" AnchorPane.topAnchor="5.0">
        <font>
            <Font name="System Bold" size="20.0"/>
        </font>
    </Button>
</AnchorPane>