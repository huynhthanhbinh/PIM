<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="120.0" prefWidth="500.0" stylesheets="/com/bht/pim/dialog/content/ExitDialogContent.css"
            xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.bht.pim.dialog.content.ExitDialogContent">

    <Button fx:id="bSubmit" defaultButton="true" layoutX="130.0" layoutY="84.0" mnemonicParsing="false"
            prefHeight="20.0" prefWidth="120.0" text="SUBMIT">
    </Button>
    <Button fx:id="bCancel" cancelButton="true" layoutX="250.0" layoutY="84.0" mnemonicParsing="false" prefHeight="20.0"
            prefWidth="120.0" text="CANCEL">
    </Button>
    <Label fx:id="lWarning" alignment="CENTER" contentDisplay="CENTER" layoutX="20.0" layoutY="36.0"
           text="Are you sure you want to exit application ?" AnchorPane.leftAnchor="20.0"
           AnchorPane.rightAnchor="20.0"/>
</AnchorPane>
